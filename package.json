{
  "name": "challenge-6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/routes/server.js",
    "dev": "nodemon dist/routes/server.js",
    "devType": "ts-node-dev --respawn --transpile-only routes/server.ts",
    "typeCompile": "npx tsc",
    "typeCompileWatch": "npx tsc --watch",
    "database:migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./config/knex.ts",
    "database:seed": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile ./config/knex.ts",
    "convertPostman": "ts-node-dev --respawn --transpile-only config/convertPostman.ts"
  },
  "_moduleAliases": {
    "@Controllers": "dist/app/controllers",
    "@Exceptions": "dist/app/exceptions",
    "@Interfaces/*": "dist/app/interfaces",
    "@Middlewares/*": "dist/app/middlewares",
    "@Models/*": "dist/app/models",
    "@Repositories/*": "dist/app/repositories",
    "@Services/*": "dist/app/services",
    "@ValidationDatas/*": "dist/app/testvalidations",
    "@Config/*": "dist/config",
    "@Routes/*": "dist/routes"
  },
  "keywords": [],
  "author": "Muhammad Fachrian Noor",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "objection": "^3.1.4",
    "pg": "^8.11.5",
    "postman-to-openapi": "^3.0.1",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.16.1",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
